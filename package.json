{
  "name": "factora",
  "version": "1.0.0",
  "description": "Create consistent, type-safe data hooks for React. Handles caching, retries, and GC so you don't have to.",
  "author": "travelr",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/travelr/factora.git"
  },
  "homepage": "https://github.com/travelr/factora",
  "bugs": {
    "url": "https://github.com/travelr/factora/issues"
  },
  "keywords": [
    "react",
    "hooks",
    "typescript",
    "data-fetching",
    "api",
    "cache",
    "caching",
    "retry",
    "retries",
    "stale-while-revalidate",
    "swr",
    "garbage-collection",
    "gc",
    "singleton",
    "store",
    "factory",
    "state-management",
    "server-state",
    "zustand"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/types/index.d.ts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/types/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./adapter/axios": {
      "import": {
        "types": "./dist/types/adapter/axios.d.ts",
        "default": "./dist/adapter/axios.mjs"
      },
      "require": {
        "types": "./dist/types/adapter/axios.d.ts",
        "default": "./dist/adapter/axios.js"
      }
    },
    "./adapter/loglevel": {
      "import": {
        "types": "./dist/types/adapter/loglevel.d.ts",
        "default": "./dist/adapter/loglevel.mjs"
      },
      "require": {
        "types": "./dist/types/adapter/loglevel.d.ts",
        "default": "./dist/adapter/loglevel.js"
      }
    }
  },
  "scripts": {
    "build": "rollup -c --configPlugin typescript --bundleConfigAsCjs",
    "coverage": "vitest run --coverage",
    "format": "prettier . --write",
    "lint": "eslint . --fix",
    "test": "vitest --config vitest.config.ts"
  },
  "peerDependencies": {
    "axios": "^1.0.0",
    "loglevel": "^1.8.0",
    "react": "^18.0.0",
    "zustand": "^4.0.0"
  },
  "peerDependenciesMeta": {
    "axios": {
      "optional": true
    },
    "loglevel": {
      "optional": true
    }
  },
  "devDependencies": {
    "@eslint/js": "^9.34.0",
    "@rollup/plugin-alias": "^5.1.1",
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.1.4",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^24.3.0",
    "@types/react": "^19.1.12",
    "@typescript-eslint/parser": "^8.41.0",
    "@vitest/coverage-v8": "^3.2.4",
    "axios": "^1.11.0",
    "eslint": "^9.34.0",
    "eslint-plugin-array-func": "^5.0.2",
    "eslint-plugin-check-file": "^3.3.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-unused-imports": "^4.2.0",
    "eslint-plugin-vitest": "^0.5.4",
    "globals": "^16.3.0",
    "husky": "^9.1.7",
    "jsdom": "^26.1.0",
    "lint-staged": "^16.1.5",
    "loglevel": "^1.9.2",
    "prettier": "^3.6.2",
    "react": "^19.1.1",
    "rollup": "^4.49.0",
    "rollup-plugin-delete": "^3.0.1",
    "rollup-plugin-dts": "^6.2.3",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.41.0",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4",
    "zustand": "^5.0.8"
  },
  "lint-staged": {
    "*.{ts,tsx,js,json,md}": "prettier --write",
    "*.{ts,tsx,js}": "eslint --fix"
  },
  "dependencies": {
    "tslib": "^2.8.1"
  }
}
